<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konskripci.xml,v 1.4 2019/03/18 00:10:14 revo Exp $">
<kap>
  <rad>konskripci</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="konskripci.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="konskripci.0i.MIL">
    <uzo tip="fak">MIL</uzo>
    <ref tip="dif" cel="varb.0i.MIL">varbi</ref>
    <ref tip="dif" cel="rekrut.0igi.MIL">rekrutigi</ref>
      <ekz>
        venis kompanio de soldatoj el la &ccirc;efsidejo de &Ubreve;ang 
        la Leono por <tld/>i niajn vila&gcirc;anojn
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
  </snc>
  <!-- formon maloftan traduku per malofta&jcirc;o se tia ekzistas -->
  <trdgrp lng="cs">
    <trd>konskribovat</trd>,
    <trd>sepsat</trd>
  </trdgrp> 
  <trdgrp lng="pl">
    <trd>pob&oacute;r wykona&cacute;</trd>,
    <trd>rekrutowa&cacute;</trd>,
    <trd>werbowa&cacute;</trd>
  </trdgrp> 
  <trd lng="ro">recrutare</trd>
  <trdgrp lng="sk">
    <trd>konskribova&tcaron;</trd>,
    <trd>sp&iacute;sa&tcaron;</trd>
  </trdgrp>
</drv>
</art>

